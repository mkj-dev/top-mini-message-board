<!DOCTYPE html>
<html>
<head>
    <title>
        <%= title %>
    </title>
    <link rel='stylesheet' href='/css/style.css' />
</head>
<body>
    <h1><%= title %></h1>
    
    <% messages.forEach(function (message){ %>
        <h2>From: <%= message.user %></h2>
        <h3><%= message.text %></h3>
        <small><%= message.added %></small>
    <% }); %>
</body>
</html>